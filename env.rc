# Default DIR
INSTALL_DIR="/opt"		# .e.g INSTALL_DIR="/opt" equivalent --prefix="/opt/unbound"

# Depends Version
#https://openssl.org/source/
OPENSSL_VERSION=3.3.1
#https://doc.libsodium.org/#downloading-libsodium
LIBSODIUM_VERSION=1.0.20-stable
#https://www.netfilter.org/pub/libmnl/
LIBMNL_VERSION=1.0.5
# For some reason hiredis 1.2.0 creates libs with version 1.1.0, causing the linker to fail. Use 1.1.0 until this is resolved https://github.com/redis/hiredis/pull/1232
#LIBHIREDIS_VERSION=1.2.0
LIBHIREDIS_VERSION=1.1.0
#https://libevent.org/
LIBEVENT_VERSION=2.1.12-stable
#https://github.com/nghttp2/nghttp2/releases
NGHTTP2_VERSION=1.62.1
#https://github.com/libexpat/libexpat/releases
EXPAT_SOURCE=2.6.2

#Required packages
required_pkgs=("autoconf" "autogen" "automake" "build-essential" "clang" "cmake" "curl" "git" "libtool" "m4" "p7zip-full" "pkg-config" "wget" "llvm")
